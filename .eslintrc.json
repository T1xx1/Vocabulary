{
   "env": {
      "browser": true,
      "es6": true
   },
   "extends": ["eslint:recommended", "plugin:react/recommended"],
   "plugins": ["react"],
   "parserOptions": {
      "ecmaFeatures": {
         "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "rules": {
      "no-empty": "warn",
      "react/prop-types": "off",
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
   },
   "settings": {
      "react": {
         "version": "detect"
      }
   }
}
