{
   "env": {
      "browser": true,
      "es2021": true
   },
   "extends": ["eslint:recommended", "plugin:react/recommended"],
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "plugins": ["react"],
   "rules": {
      "no-empty": "warn",
      "react/prop-types": "off",
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
   }
}
