{
   "name": "Vocabulary",
   "description": "Search words and learn them, get definitions, synonyms and antonyms",
   "version": "1.3.1",
   "private": true,
   "homepage": "https://t1xx1.github.io/Vocabulary",
   "dependencies": {
      "@testing-library/jest-dom": "^5.16.4",
      "@testing-library/react": "^13.2.0",
      "@testing-library/user-event": "^13.5.0",
      "purgecss": "^4.1.3",
      "react": "^18.1.0",
      "react-dom": "^18.1.0",
      "react-scripts": "^5.0.1",
      "web-vitals": "^2.1.4"
   },
   "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "deploy": "gh-pages -d build",
      "eslint": "eslint .",
      "fix": "eslint --fix .",
      "format": "prettier --write \"**/*.+(css|js|jsx|json|md|scss)\"",
      "purgecss": "purgecss --css build/static/css/*.css --content build/index.html build/static/js/*.js --output build/static/css",
      "commit": "npm run fix && npm run eslint && npm run format",
      "publish": "npm run commit && npm run build && npm run purgecss && npm run deploy"
   },
   "eslintConfig": {
      "extends": [
         "react-app",
         "react-app/jest"
      ]
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "eslint": "^8.22.0",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-react": "^7.31.0",
      "gh-pages": "^4.0.0",
      "husky": "^8.0.1",
      "prettier": "^2.7.1"
   }
}
