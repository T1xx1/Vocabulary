{
   "name": "vocabulary",
   "version": "1.4.1",
   "homepage": "https://t1xx1.github.io/Vocabulary",
   "private": true,
   "dependencies": {
      "@testing-library/jest-dom": "^5.16.4",
      "@testing-library/react": "^13.2.0",
      "@testing-library/user-event": "^13.5.0",
      "react": "^18.1.0",
      "react-dom": "^18.1.0",
      "react-scripts": "^5.0.1"
   },
   "scripts": {
      "build": "react-scripts build",
      "deploy": "gh-pages -d build",
      "purgecss": "purgecss --css build/static/css/*.css --content build/index.html build/static/js/*.js --output build/static/css",
      "start": "react-scripts start",
      "publish": "npm run build && npm run purgecss && npm run deploy"
   },
   "eslintConfig": {
      "extends": [
         "react-app",
         "react-app/jest"
      ]
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "eslint": "^5.12.0",
      "eslint-plugin-react": "^7.31.8",
      "gh-pages": "^4.0.0",
      "purgecss": "^4.1.3",
      "sass": "^1.54.9"
   }
}
